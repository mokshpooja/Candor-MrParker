#nav2 {
      display: flex;
      list-style: none;
      justify-content: space-between;
      position: relative;
      width: 90%;
      float: left;
      margin-right: 0;
      font-size: 15px;
      padding: 10px 0;
}



.navbar2 {
    margin: auto;
      width: 100%;
      display: flex;
      justify-content: center;

}
@media screen and (max-width: 960px) {
  ul#nav li a {
    font-size: 12px;
  }
  #nav2 {
    font-size: 12px;
  }
}

@media screen and (max-width: 740px) {
  #nav2 {
    display: none;
  }
}

